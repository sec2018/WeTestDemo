/* pages/trans_map/trans_map.wxss */


/*用车按钮  */
.car-d-btn{
  width: 100%;
  height: 90rpx;
  margin: 33rpx 0 20rpx 0;
  line-height: 90rpx;
  border-radius: 90rpx;
  background: #ff5f16;
  color:#fff;
  font-size: 30rpx;
  text-align: center;
}
.car-d-btn-android{
  width: 100%;
  height: 90rpx;
  margin: 33rpx 0 20rpx 0;
  line-height: 90rpx;
  border-radius: 100%;
  background: #ff5f16;
  color:#fff;
  font-size: 30rpx;
  text-align: center;
}
/*用车须知  */
.car-d-dec{
  display: flex;
  margin-top:20rpx;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color:#dadada;
  line-height: 40rpx;
  text-align: center;
}
/*预约单  */
.reservation-list{
  position: fixed;
  left:0;
  top:0;
  right:0;
  bottom:0;
}
.reservation-list-bg{
  width:100%;
  height:100%;
  background: #000000;
  opacity: 0.3;
}
.reservation-list-con{
  position: fixed;
  left:100rpx;
  top:35%;
  right:100rpx;
  height: 220rpx;
  font-size: 28rpx;
  text-align: center;
  background: #ffffff;
  color:#666;
  border-radius: 20rpx;
}
.reservation-list-tips{
  width:100%;
  background: #eeeeee;
  color:#000;
  height:80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
}