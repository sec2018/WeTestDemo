/**index.wxss**/
.con-headerimg{
  width:750rpx;
  height:400rpx;
  overflow: hidden;
}
.con-head-img{
  width:100%;
  /* height:100%; */
}
.con-btn{
  display: flex;
  font-size: 28rpx;
  color:#000;
  padding:30rpx 30rpx 0;
  justify-content: space-around;
}
.con-btncon{
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  width:300rpx;
  background: #f8f8f8;
}
.reservation-list{
  position: fixed;
  left:0;
  top:0;
  right:0;
  bottom:0;
}
.reservation-list-bg{
  width:100%;
  height:100%;
  background: #000000;
  opacity: 0.3;
}
.reservation-list-con{
  position: fixed;
  left:100rpx;
  top:35%;
  right:100rpx;
  height: 220rpx;
  font-size: 28rpx;
  text-align: center;
  background: #ffffff;
  color:#666;
  border-radius: 20rpx;
}
.reservation-list-tips{
  width:100%;
  background: #eeeeee;
  color:#000;
  height:80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
}
